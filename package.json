{
  "name": "github-analytics",
  "version": "1.0.0",
  "description": "A web application that provides beautiful charts based on data found in github issues and comments.",
  "main": "github-analytics.js",
  "scripts": {
    "start": "pm2 stop github-analytics; pm2 delete github-analytics; NODE_ENV=production pm2 start github-analytics.js",
    "dev": "NODE_ENV=local nodemon github-analytics.js",
    "debug": "NODE_ENV=local nodemon --debug github-analytics.js",
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kabitakis/github-analytics.git"
  },
  "keywords": [
    "github",
    "analytics",
    "statistics",
    "issues",
    "comments",
    "counters"
  ],
  "author": "Nikos Kampitakis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kabitakis/github-analytics/issues"
  },
  "homepage": "https://github.com/kabitakis/github-analytics",
  "dependencies": {
    "async": "^1.4.2",
    "express": "^4.13.3",
    "github": "^0.2.4",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "node-jsx": "^0.13.3",
    "react": "^0.13.3",
    "react-d3-components": "^0.4.8",
    "react-engine": "^2.2.0",
    "react-router": "^0.13.3",
    "reactify": "^1.1.1"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  }
}
